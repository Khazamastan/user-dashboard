{
  "name": "react",
  "version": "1.0.0",
  "description": "React example starter project",
  "keywords": [
    "react",
    "starter"
  ],
  "engines": {
    "npm": ">=6",
    "node": ">=12.22.12"
  },
  "main": "src/index.js",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/react": "^14.0.0",
    "axios": "^1.3.3",
    "json-server": "^0.17.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-id-generator": "3.0.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "5.3.4",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-mock-store": "^1.5.4",
    "redux-saga": "^1.2.2",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "7.13.8",
    "babel-jest": "^29.5.0",
    "concurrently": "7.6.0",
    "jest": "^29.5.0",
    "typescript": "4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
